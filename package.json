  "scripts": {
    "predeploy": "npm run build",
    "deploy": "echo 'game-datacards.eu' > ./build/CNAME && gh-pages -d build",
    "start": "craco start",
    "build": "craco build",
    "build:beta": "env-cmd -f .env.beta craco build",
    "test": "craco test",
    "test:ci": "CI=true craco test --passWithNoTests",
    "test:coverage": "CI=true craco test --passWithNoTests --coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=json-summary --coverageReporters=json",
