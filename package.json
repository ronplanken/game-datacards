    "prettier:fix": "prettier --config .prettierrc --write '**/*.{js,ts,tsx,css}'",
    "lint:fix": "yarn lint --fix",
    "pre-commit": "lint-staged",
    "start:watch": "nodemon --watch src --ext js,jsx,ts,tsx,css,less --exec \"craco start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
